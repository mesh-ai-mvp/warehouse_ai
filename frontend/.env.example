# Frontend Environment Variables

# API Configuration
VITE_API_URL=/api

# Development Settings
# Set to 'false' to disable mock fallback data in development
VITE_ENABLE_MOCK_FALLBACKS=true

# Feature Flags
# Enable real-time analytics updates
VITE_ENABLE_REALTIME_ANALYTICS=false

# Enable AI generation features (default: true)
VITE_ENABLE_AI_GENERATION=true